<CadastroPage>:
    FloatLayout:
        Image:
            source: "Images/cadastro.png"
            pos_hint: {'center_x': 0.47, 'center_y': 0.85}
            size_hint: None, None
            size: 150,150
        Label:
            text: "Usuario:"
            pos_hint: {"x":0.15, "top":0.83}
            size_hint: root.largura_label, root.altura_label
        Label:
            text: "Nome:"
            pos_hint: {"x":0.15, "top":0.78}
            size_hint: root.largura_label, root.altura_label
        Label:
            text: "E-mail:"
            pos_hint: {"x":0.15, "top":0.73}
            size_hint: root.largura_label, root.altura_label
        Label:
            text: "Telefone:"
            pos_hint: {"x":0.15, "top":0.68}
            size_hint: root.largura_label, root.altura_label
        Label:
            text: "Cpf:"
            pos_hint: {"x":0.16, "top":0.63}
            size_hint: root.largura_label, root.altura_label
        Label:
            text: "Senha:"
            pos_hint: {"x":0.15, "top":0.58}
            size_hint: root.largura_label, root.altura_label
        Label:
            text: "Repita a senha:"
            pos_hint: {"x":0.13, "top":0.53}
            size_hint: root.largura_label, root.altura_label
        TextInput:
            id: user_input
            pos_hint: {"x":0.3, "top":0.75}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            multiline: False
        TextInput:
            id: nome_input
            pos_hint: {"x":0.3, "top":0.7}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            multiline: False
        TextInput:
            id: email_input
            pos_hint: {"x":0.3, "top":0.65}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            multiline: False
        TextInput:
            id: telefone_input
            pos_hint: {"x":0.3, "top":0.6}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            multiline: False
        TextInput:
            id: cpf_input
            pos_hint: {"x":0.3, "top":0.55}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            multiline: False
        TextInput:
            id: senha_input
            pos_hint: {"x":0.3, "top":0.5}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            password: True
            multiline: False
        TextInput:
            id: senha_confirm_input
            pos_hint: {"x":0.3, "top":0.45}
            size_hint: root.largura_caixa_texto, root.altura_caixa_texto
            password: True
            multiline: False
        Button:
            text: "Cadastrar"
            pos_hint: {"x":0.4, "y":0.3}
            size_hint: root.largura_btn, root.altura_btn
            on_release: root.cadastrar_usuario(user_input.text, nome_input.text, email_input.text, telefone_input.text, cpf_input.text, senha_input.text, senha_confirm_input.text)
        Button:
            text: "<="
            size_hint: 0.05, 0.05
            pos_hint: {"x": 0, "top": 1}
            on_release: app.mudar_tela("loginpage")